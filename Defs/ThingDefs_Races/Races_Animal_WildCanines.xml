<Defs>

<!-- Coyote -->

<ThingDef ParentName="ThingBaseWolf">
    <defName>dsBTA_Coyote</defName>
    <label>coyote</label>
<description>A moderate sized, slightly genetically modified canine. Known for its intelligence, they often form hunting partnerships with other predators.</description>
    <statBases>
      <MoveSpeed>4.8</MoveSpeed>
      <ComfyTemperatureMin>-35</ComfyTemperatureMin>
      <MarketValue>300</MarketValue>
    </statBases>
	<race>
      <maxPreyBodySize>0.80</maxPreyBodySize>
      <baseBodySize>0.6</baseBodySize>
      <baseHungerRate>0.1</baseHungerRate>
      <baseHealthScale>0.80</baseHealthScale>
      <leatherDef>Leather_Wolf</leatherDef>
	 <useMeatFrom>Wolf_Timber</useMeatFrom>
      <wildness>0.8</wildness>
	  <manhunterOnDamageChance>.50</manhunterOnDamageChance>
      <gestationPeriodDays>10</gestationPeriodDays>
      <lifeExpectancy>11</lifeExpectancy>
	  			<wildBiomes>
				<AridShrubland>.2</AridShrubland>
				<BorealForest>0</BorealForest>
				<ColdBog>0</ColdBog>
				<TemperateForest>.2</TemperateForest>
				<TemperateSwamp>.2</TemperateSwamp>
				<TropicalRainforest>0</TropicalRainforest>
				<TropicalSwamp>.2</TropicalSwamp>
				<IceSheet>0</IceSheet>
				<SeaIce>0</SeaIce>
				<Tundra>0</Tundra>
				<Desert>0</Desert>
				<ExtremeDesert>0</ExtremeDesert>
			</wildBiomes>
    </race>
  </ThingDef>

