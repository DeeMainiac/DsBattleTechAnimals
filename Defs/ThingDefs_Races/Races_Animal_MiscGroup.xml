<Defs>

<!-- Cloud Cobra -->

  <ThingDef ParentName="AnimalThingBase">
  <defName>dsBTA_CloudCobra</defName>
  <label>cloud cobra</label>
  <description>A large, genetically modified cobra originally introduced to limit native wildlife on a jungle world. It has one of the deadliest venoms known to all of space.</description>
      <statBases>
      <MoveSpeed>3.5</MoveSpeed>
      <MarketValue>150</MarketValue>
      <ComfyTemperatureMin>0</ComfyTemperatureMin>
      <ComfyTemperatureMax>80</ComfyTemperatureMax>
    </statBases>  
	    <uiIconScale>1.1</uiIconScale>
	<tools>
      <li>
        <label>venom-fangs</label>
        <capacities>
          <li>ToxicBite</li>
        </capacities>
        <power>15</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>15</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>3</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <comps>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>dsBTA_EggCloudCobraFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>10</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>1~2</eggCountRange>
      </li>
    </comps>
      <race>
      <body>Snake</body>
      <predator>true</predator>
      <maxPreyBodySize>0.35</maxPreyBodySize>
      <baseBodySize>0.25</baseBodySize>
      <baseHungerRate>0.07</baseHungerRate>
      <baseHealthScale>0.5</baseHealthScale>
      <foodType>CarnivoreAnimal, OvivoreAnimal</foodType>
      <leatherDef>Leather_Lizard</leatherDef>
      <useMeatFrom>Cobra</useMeatFrom>
      <wildness>0.75</wildness>
      <petness>0.05</petness>
      <manhunterOnTameFailChance>0.10</manhunterOnTameFailChance>
      <manhunterOnDamageChance>0.50</manhunterOnDamageChance>
      <nameOnTameChance>1</nameOnTameChance>
      <lifeExpectancy>20</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.15</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.5</minAge>
          <soundWounded>Pawn_Cobra_Wounded</soundWounded>
          <soundDeath>Pawn_Cobra_Death</soundDeath>
          <soundCall>Pawn_Cobra_Call</soundCall>
          <soundAngry>Pawn_Cobra_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
			<wildBiomes>
				<AridShrubland>0</AridShrubland>
				<BorealForest>0</BorealForest>
				<ColdBog>0</ColdBog>
				<TemperateForest>.2</TemperateForest>
				<TemperateSwamp>0</TemperateSwamp>
				<TropicalRainforest>.2</TropicalRainforest>
				<TropicalSwamp>.2</TropicalSwamp>
				<IceSheet>0</IceSheet>
				<SeaIce>0</SeaIce>
				<Tundra>0</Tundra>
				<Desert>0</Desert>
				<ExtremeDesert>0</ExtremeDesert>
			</wildBiomes>
    </race>
      <tradeTags>
      <li>AnimalUncommon</li>
      <li>AnimalFighter</li>
    </tradeTags>
  </ThingDef>

